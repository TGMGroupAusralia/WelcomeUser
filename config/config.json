{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "welcomeuser-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/Welcomeuser/WelcomeuserWebPart.js",
          "manifest": "./src/webparts/Welcomeuser/WelcomeuserWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": { "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js", "globalName": "jQuery" },
    "angular": { "path": "https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.6.9/angular.min.js", "globalName": "angular" }
  },
  "localizedResources": {
    "WelcomeuserWebPartStrings": "lib/webparts/Welcomeuser/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "sp-client-custom-fields/strings": "node_modules/sp-client-custom-fields/lib/loc/{locale}.js"
  }
}
